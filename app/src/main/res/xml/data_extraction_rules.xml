<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- This app participates in cloud backup, default is include all files -->
        <include domain="file" />
        <include domain="database" />
        <include domain="sharedpref" />
        <exclude domain="file" path="no_backup/" />
    </cloud-backup>

    <device-transfer>
        <!-- This app participates in device transfer, default is include all files -->
        <include domain="file" />
        <include domain="database" />
        <include domain="sharedpref" />
        <exclude domain="file" path="no_backup/" />
    </device-transfer>
</data-extraction-rules>
